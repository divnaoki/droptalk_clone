{
    "version": 2,
    "builds": [
        {
            "src": "droptalk/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "droptalk/wsgi.py"
        }
    ],
    "env": {
        "PYTHONPATH": ".",
        "DJANGO_SETTINGS_MODULE": "droptalk.settings"
    }
} 